
{
  "name": "capllm",
  "version": "1.0.0",
  "description": "A CAP & AI Example using the CAP LLM Plugin.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.14.0",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "cap-llm-plugin": "^1.3.1",
    "express": "^4",
    "langchain": "^0.1.36",
    "@sap/cds-dk": "^7"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "auth": {
      "kind": "xsuaa" 
    },
      "db": {
        "[production]": {
          "kind": "hana"
        },
        "[development]": {
          "kind": "sql"
        }
      }
    }
    ,
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
